<div class="home_buttons">

    <button class="btn btn-primary" type="submit">
    <%= link_to 'Home', home_page_path %>
    </button>

    <button class="btn btn-primary" type="submit">
    <%= link_to 'Add New Meme', new_meme_path %>
    </button>

    <button class="btn btn-primary" type="submit">
    <%= link_to 'Show All Memes', memes_path %>
    </button>

    <button class="btn btn-primary" type="submit">
    <%= link_to 'Register New User', new_user_registration_path %>
    </button>

<% if user_signed_in? %>
    <button class="btn btn-primary" type="submit">
    <%= link_to 'Log Out', destroy_user_session_path, method: :delete %>
    </button>
<% end %>

<% if !user_signed_in? %>
    <button class="btn btn-primary" type="submit">
    <%= link_to 'Log In', new_user_session_path %>
    </button>
<% end %>